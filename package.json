    "json-server": "^0.17.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.20",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "typescript": "^4.7.4",
    "webpack": "^5.75.0"
